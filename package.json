{
  "name": "coffeekraken-sugar",
  "description": "A little sugar in your codebase",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "dist": "npm run dist.build",
    "dist.build": "concurrently --kill-others \"npm run dist.build.js\"",
    "dist.build.js": "babel src -d dist",
    "dist.watch": "concurrently --kill-others \"npm run dist.watch.js\"",
    "dist.watch.js": "nodemon -w src -e js -x \"npm run dist\"",
		"demo": "npm run demo.build && open ./demo/index.html",
    "demo.build": "concurrently --kill-others \"npm run demo.build.css\" \"npm run demo.build.js\"",
    "demo.build.css": "node-sass demo/src/sass/style.scss demo/dist/css/style.css",
    "demo.build.js": "npm run dist.build.js && webpack",
    "demo.watch": "concurrently --kill-others \"npm run demo.watch.css\" \"npm run demo.watch.css\"",
		"demo.watch.css": "nodemon -w demo/src -w src -e scss -x \"npm run demo.build.css\"",
    "demo.watch.js": "nodemon -w demo/src -w src -e js -x \"npm run demo.build.js\"",
    "install": "./scripts/install.sh"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:olivierbossel/sugar.git"
  },
  "author": "Olivier Bossel <olivier.bossel@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "custom-event": "^1.0.0",
    "fastdom": "^1.0.5",
    "flatpickr": "^2.1.1",
    "form-serialize": "^0.7.1",
    "google-maps": "^3.2.1",
    "javascript-detect-element-resize": "^0.5.3",
    "js-cookie": "^2.1.3",
    "lodash": "^4.15.0",
    "morphdom": "^2.1.2",
    "nouislider": "^8.5.1",
    "rxjs": "^5.0.0-beta.10",
    "sharer.npm.js": "0.0.10",
    "simple-ajax": "^2.6.0",
    "tocca": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.16.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-dataset": "git+https://github.com/ryanbahniuk/babel-plugin-dataset.git",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-proto-to-assign": "^6.5.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "concurrently": "^3.1.0",
    "gridle": "https://github.com/olivierbossel/gridle.git#alpadia",
    "node-sass": "^3.4.2",
    "nodemon": "^1.11.0",
    "sassyjson": "^1.1.8",
    "vinyl-named": "^1.1.0",
    "webpack": "^1.13.3"
  }
}
