{
  "name": "coffeekraken-sugar",
  "description": "A little sugar in your codebase",
  "version": "1.0.7",
  "homepage": "http://coffekraken.io",
  "main": "index.js",
  "watch": {
    "dist:js": {
      "paths": "src/**/*.js"
    }
  },
  "scripts": {
    "dist": "rm -rf dist && npm run dist:js",
    "dist:js": "babel src -d dist",
    "install": "node ./scripts/install.js",
    "doc": "rm -rf doc/src && npm run doc:scss && npm run doc:js",
    "doc:scss": "coffeekraken-docblock-to-markdown -f 'src/**/*.scss' -d doc",
    "doc:js": "coffeekraken-docblock-to-markdown -f 'src/**/*.js' -d doc",
    "test": "jest --watch",
    "start": "coffeekraken-scripts-stack -i dist -i install -i start",
    "changelog": "git-changelog --version_name $npm_package_version  --app_name $npm_package_name --intro \"$npm_package_description\" --logo \".resources/coffeekraken-logo.jpg\""
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:coffeekraken/sugar.git"
  },
  "author": "Olivier Bossel <olivier.bossel@gmail.com>",
  "license": "MIT",
  "jest": {
    "roots": ["tests"]
  },
  "dependencies": {
    "custom-event": "^1.0.0",
    "domready": "^1.0.8",
    "es6-object-assign": "^1.1.0",
    "fastdom": "^1.0.5",
    "form-serialize": "^0.7.1",
    "lodash": "^4.15.0",
    "modularscale-sass": "^2.1.1",
    "rxjs": "^5.0.0-beta.10",
    "simple-ajax": "^2.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-default": "^7.0.0-alpha.20",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^3.5.0",
    "coffeekraken-docblock-to-markdown": "0.0.2",
    "coffeekraken-scripts-stack": "^1.0.1",
    "fs-extra": "^6.0.1",
    "git-changelog": "^1.1.2",
    "jest": "^23.0.1",
    "karma": "^2.0.2",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^3.0.0",
    "node-sass": "^3.4.2",
    "regenerator-runtime": "^0.11.1",
    "webcomponents.js": "^0.7.24",
    "webpack": "^4.0.0"
  }
}
